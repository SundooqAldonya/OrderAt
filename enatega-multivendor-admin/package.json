{
  "name": "enatega-multivendor",
  "version": "2.0.0",
  "description": "Enatega MultiVendor dashboard",
  "main": "index.js",
  "homepage": "https://multivendor-admin.ninjascode.com/",
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "start": "cross-env REACT_APP_ENV=development react-scripts --openssl-legacy-provider start",
    "start:staging": "cross-env REACT_APP_ENV=staging react-scripts --openssl-legacy-provider start",
    "start:prod": "cross-env REACT_APP_ENV=production react-scripts --openssl-legacy-provider start",
    "build": "cross-env REACT_APP_ENV=production react-scripts --openssl-legacy-provider build",
    "build:dev": "cross-env REACT_APP_ENV=development react-scripts --openssl-legacy-provider build",
    "build:staging": "cross-env REACT_APP_ENV=staging react-scripts --openssl-legacy-provider build",
    "build:production": "cross-env REACT_APP_ENV=production react-scripts --openssl-legacy-provider build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "format": "prettier --write '**/*.{js,jsx}'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "npm run format",
      "npm run lint:fix"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "@apollo/client": "^4.0.7",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@googlemaps/markerclusterer": "^2.6.2",
    "@mui/icons-material": "^7.3.4",
    "@mui/material": "^7.3.4",
    "@mui/styles": "^6.4.8",
    "@mui/system": "^7.3.3",
    "@react-google-maps/api": "^2.20.7",
    "@sentry/react": "^10.20.0",
    "@sentry/tracing": "^7.120.4",
    "@wojtekmaj/react-timerange-picker": "^7.0.0",
    "apollo-upload-client": "^19.0.0",
    "chart.js": "^4.5.1",
    "date-fns": "^4.1.0",
    "env-cmd": "^11.0.0",
    "firebase": "^12.4.0",
    "formik": "^2.4.6",
    "graphql": "^16.11.0",
    "husky": "^8.0.3",
    "i18next": "^25.6.0",
    "moment": "2.30.1",
    "nouislider": "15.8.1",
    "react": "^19.2.0",
    "react-chartjs-2": "^5.3.0",
    "react-data-table-component": "^7.7.0",
    "react-datepicker": "^8.8.0",
    "react-datetime": "3.3.1",
    "react-dom": "^19.2.0",
    "react-google-autocomplete": "^2.7.5",
    "react-i18next": "^16.0.1",
    "react-loader-spinner": "^7.0.3",
    "react-notifications": "^1.7.4",
    "react-places-autocomplete": "^7.3.0",
    "react-router-dom": "7.9.4",
    "react-scripts": "^5.0.1",
    "sass": "^1.93.2",
    "source-map-explorer": "^2.5.3",
    "styled-components": "^6.1.19",
    "subscriptions-transport-ws": "^0.11.0",
    "use-places-autocomplete": "^4.0.1",
    "validate.js": "^0.13.1",
    "yup": "^1.7.1"
  },
  "devDependencies": {
    "@types/googlemaps": "3.43.3",
    "@types/markerclustererplus": "2.1.33",
    "@types/react": "^19.2.2",
    "cross-env": "^10.1.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.4",
    "prettier": "3.6.2",
    "prettier-config-standard": "^7.0.0",
    "typescript": "4.9.5"
  }
}
